---
layout: null
---
{% assign talks = site.data.3dtalks | sort: 'date' %}
BEGIN:VCALENDAR
PRODID:-//3D geoinformation group at TU Delft//NONSGML 3D talks calendar version 1.0//EN
VERSION:2.0
BEGIN:VTIMEZONE
TZID:Europe/Amsterdam
BEGIN:DAYLIGHT
TZOFFSETFROM:+0100
TZOFFSETTO:+0200
DTSTART:19810329T020000
RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=-1SU
TZNAME:CEST
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:+0200
TZOFFSETTO:+0100
DTSTART:19961027T030000
RRULE:FREQ=YEARLY;BYMONTH=10;BYDAY=-1SU
TZNAME:CET
END:STANDARD
END:VTIMEZONE
{% for i in talks %}
BEGIN:VEVENT
SUMMARY:3D talk
DTSTAMP;TZID=Europe/Amsterdam:{{ i.date | date: '%Y%m%d' }}T{{ i.time | slice: 0, 2 }}{{ i.time | slice: 3, 2 }}00
DTSTART;TZID=Europe/Amsterdam:{{ i.date | date: '%Y%m%d' }}T{{ i.time | slice: 0, 2 }}{{ i.time | slice: 3, 2 }}00
DTEND;TZID=Europe/Amsterdam:{{ i.date | date: '%Y%m%d' }}T{{ i.time | slice: 6, 2 }}{{ i.time | slice: 9, 2 }}00
DESCRIPTION:{{ i.name }}: {{ i.title }}
LOCATION:{{ i.room }}
BEGIN:VALARM
  TRIGGER:-PT3H
  ACTION:DISPLAY
  DESCRIPTION:3D talk
END:VALARM
END:VEVENT
{% endfor %}
END:VCALENDAR
